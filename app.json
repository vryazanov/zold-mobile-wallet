{
  "expo": {
    "name": "Zold Wallet",
    "slug": "zold-mobile-wallet",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "entryPoint": "./src/index.js",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zold.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zold-wallet.mobile"
    },
    "description": "It's a mobile implementation of Zold Wallet. See https://wts.zold.io for details.",
    "githubUrl": "https://github.com/vryazanov/zold-mobile-wallet"
  }
}